{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "class-methods-use-this": "off",
        "import/extensions": "off",
        "import/prefer-default-export":"off",
        "import/no-unresolved":"off",
        "no-plusplus": [2, {"allowForLoopAfterthoughts": true}]
    }
}
