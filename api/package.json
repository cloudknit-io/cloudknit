{
  "name": "cloudknit-api",
  "version": "0.1.0",
  "description": "",
  "author": "CloudKnit Inc",
  "private": true,
  "license": "Apache License 2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "ts-node test/e2e.ts",
    "test:step": "STEPCI_DISABLE_ANALYTICS=true stepci run stepci.yml --env startTime=$(node --eval=\"console.log(new Date(Date.now()))\") --env endTime=$(node --eval=\"console.log(new Date(Date.now() + 86400))\")",
    "test:step-one": "STEPCI_DISABLE_ANALYTICS=true stepci run step-1org.yml --env startTime=$(node --eval=\"console.log(new Date(Date.now()))\") --env endTime=$(node --eval=\"console.log(new Date(Date.now() + 86400))\")",
    "typeorm": "ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts",
    "typeorm:dev": "DOTENV_CONFIG_PATH=.env.dev npx ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./dist/typeorm/data-source.js",
    "typeorm:prod": "DOTENV_CONFIG_PATH=.env.prod ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts",
    "typeorm:bootstrap": "DOTENV_CONFIG_PATH=.env.bootstrap ts-node-dev -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.18.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "aws-sdk": "^2.952.0",
    "cache-manager": "^5.1.4",
    "cache-manager-ioredis-yet": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.2.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^4.0.0",
    "source-map-support": "^0.5.20",
    "stepci": "^2.6.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
